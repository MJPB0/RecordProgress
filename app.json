{
  "expo": {
    "name": "recordprogress",
    "slug": "recordprogress",
    "scheme": "recordprogress",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": true,
      "bundleIdentifier": "mjpb.recordprogress"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "mjpb.recordprogress"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ]
    ],
    "owner": "mjpb",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "49adb900-d6df-44d4-943d-6aab30c550fb"
      }
    }
  }
}
